{
  "name": "codex-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "concurrently --names \"NEXT,INNGEST\" --prefix-colors \"blue,green\" \"INNGEST_DEV=1 next dev --turbopack\" \"npx inngest-cli@latest dev -u http://localhost:3000/api/inngest\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ai": "playwright test tests/e2e/ai-powered-advanced.spec.ts",
    "test:e2e:visual": "playwright test tests/e2e/visual-regression-ai.spec.ts",
    "test:e2e:stagehand": "STAGEHAND_DEBUG=true playwright test tests/e2e/ai-powered-advanced.spec.ts tests/e2e/visual-regression-ai.spec.ts",
    "test:all": "bun run test:unit && bun run test:integration && bun run test:e2e",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "npx http-server storybook-static",
    "build-storybook": "storybook build",
    "test-storybook": "vitest --project=storybook",
    "test-storybook:ci": "test-storybook",
    "analyze": "next build --analyze",
    "audit": "bun audit --audit-level=moderate",
    "audit:fix": "bun audit fix",
    "security": "bun audit && npx audit-ci --moderate",
    "deps:check": "npx depcheck",
    "deps:update": "npx bun-check-updates -i",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "quality": "bun run lint && bun run type-check && bun run test:coverage && bun run security",
    "pre-commit": "bun run format && bun run lint:fix && bun run type-check",
    "pre-push": "bun run test:all && bun run security",
    "monitor:performance": "node scripts/performance-monitor.js",
    "monitor:vitals": "node scripts/vitals-monitor.js",
    "bundle:analyze": "npx bundlesize",
    "prepare": "husky",
    "stagehand:setup": "node scripts/stagehand-setup.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@browserbasehq/sdk": "^2.6.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@hono/node-server": "^1.16.0",
    "@inngest/realtime": "^0.3.1",
    "@openauthjs/openauth": "^0.4.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@testing-library/dom": "^10.4.0",
    "@vibe-kit/sdk": "^0.0.21",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.6",
    "global": "^4.4.0",
    "hono": "^4.8.5",
    "inngest": "^3.40.1",
    "inngest-cli": "^1.8.2",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "open": "^10.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "stagehand": "^1.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@browserbasehq/stagehand": "^2.4.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@hookform/resolvers": "^5.1.1",
    "@playwright/test": "^1.54.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/nextjs": "^9.0.17",
    "@storybook/test-runner": "^0.23.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "4.3.1",
    "concurrently": "^9.2.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-storybook": "^9.0.17",
    "husky": "9.1.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
    "react-hook-form": "^7.60.0",
    "semantic-release": "24.2.7",
    "storybook": "^9.0.17",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
