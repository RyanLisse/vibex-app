{
  "name": "codex-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "concurrently --names \"NEXT,INNGEST\" --prefix-colors \"blue,green\" \"INNGEST_DEV=1 next dev --turbopack\" \"npx inngest-cli@latest dev -u http://localhost:3000/api/inngest\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "bun run test:unit && bun run test:integration",
    "typecheck": "tsc --noEmit",
    "test:watch": "concurrently --names \"LOGIC,COMPONENTS\" --prefix-colors \"blue,green\" \"bun run test:unit:logic:watch\" \"bun run test:unit:components:watch\"",
    "test:coverage": "bun run test:unit:coverage && bun run test:integration:coverage && bun run test:coverage:merge",
    "test:coverage:merge": "node scripts/merge-coverage.js",
    "test:coverage:all": "bun run test:unit:coverage && bun run test:integration:coverage && bun run test:e2e",
    "test:ui": "vitest --ui --config=vitest.integration.config.ts",
    "test:unit": "bun run test:unit:logic",
    "test:components": "vitest run --config vitest.final.config.ts",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:unit:logic": "bun test lib/telemetry.test.ts lib/stream-utils.test.ts lib/github-api.test.ts lib/auth/index.test.ts lib/message-handlers.test.ts lib/auth.test.ts lib/container-types.test.ts lib/utils.test.ts lib/inngest.test.ts lib/inngest-isolated.test.ts lib/inngest-standalone.test.ts lib/inngest.unit.test.ts --timeout=10000",
    "test:inngest": "NODE_ENV=test INNGEST_EVENT_KEY=test-event-key VITEST_INNGEST_TESTS=true bun test lib/inngest.test.ts lib/inngest-isolated.test.ts lib/inngest-standalone.test.ts app/actions/inngest.test.ts app/api/inngest/route.test.ts --timeout=5000",
    "test:unit:logic:watch": "bun test --watch --timeout=10000",
    "test:unit:logic:coverage": "bun test --coverage --timeout=10000",
    "test:unit:components": "echo 'React component tests moved to integration layer or skipped - using Bun for unit tests only'",
    "test:unit:components:vitest": "vitest run --config=vitest.unit.components.config.ts --reporter=verbose",
    "test:unit:components:watch": "vitest --watch --config=vitest.unit.config.ts --reporter=verbose",
    "test:unit:components:coverage": "vitest run --coverage --config=vitest.unit.config.ts --reporter=verbose",
    "test:unit:coverage": "bun run test:unit:logic:coverage && bun run test:unit:components:coverage",
    "test:unit:coverage:report": "bun run test:unit:coverage && open coverage/vitest-components/index.html",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --watch --config=vitest.integration.config.ts --reporter=verbose",
    "test:integration:coverage": "vitest run --coverage --config=vitest.integration.config.ts --reporter=verbose",
    "test:inngest:standalone": "bun test lib/inngest-standalone.test.ts --timeout=5000",
    "test:inngest:watch": "NODE_ENV=test INNGEST_EVENT_KEY=test-event-key VITEST_INNGEST_TESTS=true bun test --watch lib/inngest.test.ts lib/inngest-isolated.test.ts lib/inngest-standalone.test.ts --timeout=5000",
    "test:integration:coverage:report": "bun run test:integration:coverage && open coverage/vitest-integration/index.html",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ai": "playwright test tests/e2e/ai-powered-advanced.spec.ts",
    "test:e2e:visual": "playwright test tests/e2e/visual-regression-ai.spec.ts",
    "test:e2e:stagehand": "STAGEHAND_DEBUG=true playwright test tests/e2e/ai-powered-advanced.spec.ts tests/e2e/visual-regression-ai.spec.ts",
    "test:all": "bun run test:unit:logic && bun run test:unit:components && bun run test:integration && bun run test:e2e",
    "test:safe": "bun run test:unit:logic && bun run test:unit:components && bun run test:integration",
    "test:fast": "bun run test:unit:logic && bun run test:integration",
    "test:all:watch": "concurrently --names \"LOGIC,COMPONENTS,INTEGRATION\" --prefix-colors \"blue,green,yellow\" \"bun run test:unit:logic:watch\" \"bun run test:unit:components:watch\" \"bun run test:integration:watch\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "npx http-server storybook-static",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --browsers chromium",
    "test-storybook:ci": "test-storybook",
    "analyze": "next build --analyze",
    "audit": "bun audit --audit-level=moderate",
    "audit:fix": "bun audit fix",
    "security": "bun audit && npx audit-ci --moderate",
    "deps:check": "npx depcheck",
    "deps:update": "npx bun-check-updates -i",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "quality": "bun run lint && bun run type-check && bun run test:coverage && bun run security",
    "pre-commit": "bun run check:fix && bun run type-check",
    "pre-push": "bun run test && bun run security",
    "monitor:performance": "node scripts/performance-monitor.js",
    "monitor:vitals": "node scripts/vitals-monitor.js",
    "bundle:analyze": "npx bundlesize",
    "prepare": "husky",
    "stagehand:setup": "node scripts/stagehand-setup.js",
    "fix:typescript": "bun run scripts/fix-typescript-errors.ts",
    "fix:vitest": "bun run scripts/fix-vitest-config.ts",
    "fix:components": "bun run scripts/fix-component-props.ts",
    "fix:all": "bun run scripts/automation-master.ts",
    "test:no-hanging": "bun run fix:vitest && bun run test",
    "quality:auto": "bun run fix:all && bun run check:fix && bun run type-check",
    "automation:status": "bunx tsc --noEmit | grep -c 'error TS' || echo '0 TypeScript errors'",
    "test:coverage:report": "bun run test:coverage && open coverage/final-report/index.html",
    "test:coverage:validate": "node scripts/validate-coverage.js",
    "test:coverage:clean": "rm -rf coverage && mkdir -p coverage",
    "db": "bun run db/cli.ts",
    "db:migrate": "bun run db/cli.ts migrate",
    "db:rollback": "bun run db/cli.ts rollback",
    "db:status": "bun run db/cli.ts status",
    "db:create": "bun run db/cli.ts create",
    "db:health": "bun run db/cli.ts health",
    "db:init": "bun run db/cli.ts init",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "migration": "bun run scripts/migration-cli.ts",
    "migration:status": "bun run scripts/migration-cli.ts status",
    "migration:migrate": "bun run scripts/migration-cli.ts migrate",
    "migration:backup:list": "bun run scripts/migration-cli.ts backup:list",
    "migration:backup:create": "bun run scripts/migration-cli.ts backup:create",
    "migration:test": "bun run scripts/test-data-migration.ts",
    "migration:test:basic": "bun run scripts/test-migration-basic.ts",
    "electric:test": "bun run scripts/test-electric-integration.ts",
    "demo:voice-brainstorm": "bun run scripts/demo-voice-brainstorm.ts",
    "demo:voice-brainstorm:perf": "bun run scripts/demo-voice-brainstorm.ts --performance",
    "wasm:build": "cd wasm-modules/vector-search && ./build.sh",
    "wasm:test": "bun run lib/wasm/modules/test-vector-search.ts"
  },
