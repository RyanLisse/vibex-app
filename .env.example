# Letta API Configuration
LETTA_API_KEY=your_letta_api_key_here
LETTA_BASE_URL=https://api.letta.com
LETTA_PROJECT_ID=your_project_id_here

# OpenAI Configuration (for voice transcription)
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Google AI Configuration (for Gemini)
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Database Configuration
DATABASE_URL=your_database_url_here

# ElectricSQL Configuration
ELECTRIC_URL=your_electric_url_here
ELECTRIC_AUTH_TOKEN=your_electric_auth_token_here
ELECTRIC_AUTH_ENDPOINT=/api/auth/electric
ELECTRIC_USER_ID=your_user_id_here
ELECTRIC_API_KEY=your_api_key_here
ELECTRIC_API_KEYS=api_key_1,api_key_2,api_key_3
ELECTRIC_ADMIN_USERS=admin_user_1,admin_user_2

# ElectricSQL Sync Configuration
ELECTRIC_SYNC_INTERVAL=1000
ELECTRIC_MAX_RETRIES=3
ELECTRIC_RETRY_BACKOFF=1000
ELECTRIC_MAX_QUEUE_SIZE=1000
ELECTRIC_CONNECTION_TIMEOUT=10000
ELECTRIC_HEARTBEAT_INTERVAL=30000

# ElectricSQL Local Database
ELECTRIC_LOCAL_DB_PATH=idb://electric-local

# Authentication
AUTH_SECRET=your_auth_secret_here

# Logging Configuration
# Log level: error, warn, info, debug, trace
LOGGING_LEVEL=info

# Service Information
SERVICE_NAME=codex-clone
SERVICE_VERSION=1.0.0

# Console Logging
LOGGING_CONSOLE_ENABLED=true
LOGGING_CONSOLE_LEVEL=debug

# File Logging
LOGGING_FILE_ENABLED=true
LOGGING_FILE_PATH=logs/app.log
LOGGING_ERROR_FILE_PATH=logs/error.log
LOGGING_FILE_MAX_SIZE=10485760
LOGGING_FILE_MAX_FILES=5
LOGGING_FILE_LEVEL=info

# HTTP Logging (for external services)
LOGGING_HTTP_ENABLED=false
LOGGING_HTTP_HOST=
LOGGING_HTTP_PORT=80
LOGGING_HTTP_PATH=/logs
LOGGING_HTTP_SSL=false
LOGGING_HTTP_LEVEL=warn

# Performance and Sampling
LOGGING_SAMPLING_ENABLED=false
LOGGING_SAMPLING_RATE=0.1
LOGGING_HIGH_VOLUME_THRESHOLD=1000
LOGGING_TRACK_OPERATIONS=true
LOGGING_SLOW_THRESHOLD=1000

# Security and Redaction
LOGGING_REDACTION_ENABLED=true
LOGGING_REDACTION_FIELDS=custom_secret,internal_token

# Other existing environment variables...