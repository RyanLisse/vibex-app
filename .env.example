# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# AI & Testing Services
# OpenAI API key for AI features and Stagehand testing
OPENAI_API_KEY=your_openai_api_key_here

# Browserbase for Stagehand AI testing (optional)
BROWSERBASE_API_KEY=your_browserbase_api_key
BROWSERBASE_PROJECT_ID=your_browserbase_project_id

# GitHub Integration (optional)
# For local development, create a GitHub OAuth app with callback: http://localhost:3000/auth/callback
# For production, use callback: https://vibex-app.vercel.app/auth/callback
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_TOKEN=your_github_personal_access_token
# Optional: Override the redirect URI (useful for testing)
# GITHUB_REDIRECT_URI=http://localhost:3000/auth/callback

# Database (if using)
DATABASE_URL=your_database_connection_string

# Inngest (event processing)
# Get these from https://app.inngest.com/
INNGEST_SIGNING_KEY=your_inngest_signing_key
INNGEST_EVENT_KEY=your_inngest_event_key
# Set to 1 for local development to bypass signature verification
INNGEST_DEV=

# Monitoring & Analytics (optional)
VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# OpenTelemetry Configuration (optional)
# Enable telemetry tracing for VibeKit operations
OTEL_ENABLED=false
# OTLP trace export endpoint (examples below)
# Jaeger: http://localhost:14268/api/traces
# Zipkin: http://localhost:9411/api/v2/spans
# DataDog: https://trace.agent.datadoghq.com/v0.3/traces
# New Relic: https://otlp.nr-data.net:4317
# Honeycomb: https://api.honeycomb.io/v1/traces
# Grafana Tempo: http://localhost:4317
OTEL_ENDPOINT=http://localhost:4317
# Service identification
OTEL_SERVICE_NAME=codex-clone
OTEL_SERVICE_VERSION=1.0.0
# Authentication header for telemetry backend (e.g., "Bearer your-api-key")
OTEL_AUTH_HEADER=
# Trace sampling ratio (0.0 to 1.0, where 1.0 = 100% sampling)
OTEL_SAMPLING_RATIO=1.0

# Development & Testing
STAGEHAND_DEBUG=false
DEBUG_DOM=false
VERBOSE=false

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000